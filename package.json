    "node": "^20.17.0",
    "npm": "^10.8.3"
  },
  "dependencies": {
    "gl-matrix": "^3.4.3"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.7",
    "browser-sync": "^3.0.2",
    "concurrently": "^9.0.1",
    "jsdoc": "^4.0.3",
    "p5": "^1.11.0",
    "rollup": "^4.22.2",
    "tap": "^16.3.10"
  }
